<!DOCTYPE html>
<html>
<head>
  <title>Links</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if current_user %>
Hello <%= current_user.username %>
<%= button_to "Sign Out", session_url, method: :delete %>
<% else %>
<%= link_to "Sign In", new_session_url %>
<% end %>

<%= render 'shared/errors' %>

<%= yield %>

</body>
</html>
